mydb=# WITH numbered_students AS (
  SELECT name, ROW_NUMBER() OVER (ORDER BY name) AS new_id
  FROM students
)
UPDATE students s
SET student_id = ns.new_id
FROM numbered_students ns
WHERE s.name = ns.name;
UPDATE 3
